@Title=CrackLockedFiles
@Echo 
cls
Set choice=A
Set choice=B
Set choice=C 
/p A="letter abcABC"
/p B="NUMBER 123"
/p C="BRUT-FORCE abc'ABC'123'@^$&"
}
choice of user;
%input%=A
%input%=B
%input%=C 
}
Echo ENTER THE FIGHT "A'B'C"
REM
}
'if' %A%=goto A
'if' %B%=goto B
'if' %C%=goto C
}
:A
Start .
%RANDOM% "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ"
goto infinite
Find PASSWORD=%PSWD%
Echo PASSWORD IS %PSWD%
goto stop
}
:B
Start .
%RANDOM% "0123456789"  
goto infinite
Find PASSWORD=%PSWD%
Echo PASSWORD IS %PSWD%
goto stop
}
:C
Start .
%RANDOM% "abcdefghijklmnopqrstuvwxyzABCDEFGHIJKLMNOPQRSTUVWXYZ0123456789"
goto infinite 
Find PASSWORD=%PSWD%
Echo PASSWORS IS %PSWD%
goto stop
